<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.BorderPane?>
<VBox alignment="TOP_LEFT" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="de.schmiereck.col.HelloController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>

    <HBox>
        <Button text="Hello!" onAction="#onHelloButtonClick"/>
        <Button text="Hello2!" onAction="#onHelloButtonClick"/>
    </HBox>
    <Label fx:id="welcomeText"/>
    <BorderPane fx:id="welcomePane">
        <Canvas fx:id="welcomeCanvas" height="100.0" width="100.0"/>
    </BorderPane>

</VBox>
